#include <bits/stdc++.h>
 
using namespace std;
 
int main()
{
  int n;
  cin>>n;
char arrc[n];
int arrt[n];
char v[n];
for(int i=1; i<=n; i++){
    cin>>arrc[i];
 
}
for(int i=1; i<=n; i++){
    cin>>arrt[i];
}
for(int i=1; i<=n; i++){
    v[arrt[i]]=arrc[i];
}
for(int i=1; i<=n; i++){
    cout<<v[i];
}
 
 
}
