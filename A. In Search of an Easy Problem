#include<bits/stdc++.h>
using namespace std;
    main(){
    int n;
    cin>>n;
    int a[n];
     int c1=0;
    for(int i=0; i<n; i++){
            cin>>a[i];
            if(a[i]==1)
                c1++;


    }

    if(c1>=1)
        cout<<"HARD"<<endl;
    else
        cout<<"EASY"<<endl;







    }
